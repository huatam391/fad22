<% provide :title, t(".home") %>
<div class="center jumbotron">
  <h1><%= t(".fastfood") %></h1>
  <%= link_to t(".signup"), new_user_registration_path,
    class: "btn btn-lg btn-primary" unless user_signed_in? %>
</div>
<% if user_signed_in? && current_user.normal? %>
